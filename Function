// FUNCTION
double luasLingkaran(double r){
		return M_PI*r*r;
	}
	
int main(){
	double r;
	
	cin >> r;
	cout << "Luas lingkaran = " << luasLingkaran(r);
	
}

void absen(string nama, string NPM){
	cout << nama << " (" << NPM << ")";
}

int main(){
	string nama, NPM;
	cout << "Masukkan nama: ";
	getline(cin, nama);
	cout << "Masukkan NPM: ";
	cin >> NPM;
	
	absen(nama, NPM);
}
